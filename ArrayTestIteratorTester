/*Written By Archie Archbold
 * Tester to use with ArrayTest.java*/

public class ArrayTestIteratorTester {

	public static void main(String[] args) {
		ArrayTest <Integer>at = new ArrayTest();
		
		at.add(2);
		at.add(4);
		at.add(7);
		
		System.out.println(at.get(0));
		System.out.println(at.get(1));
		System.out.println(at.get(2));
		
		for(int i=0; i<at.size();i++){
			System.out.println(at.get(i));
		}
		
		for(int x: at){
			System.out.println(x);
		}
		
		at.printAll();
	}
	
}
